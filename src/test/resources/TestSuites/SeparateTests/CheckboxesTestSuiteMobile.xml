<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="CheckboxesTestSuite" verbose="1">

    <test name="CheckboxesTests-Android">
        <parameter name="deviceName" value="Samsung Galaxy S8+"></parameter>
        <classes>
            <class name="com.herokuapp.theinternet.checkboxes.CheckboxesTests"/>
        </classes>
    </test>

    <test name="CheckboxesTests-IPhone">
        <parameter name="deviceName" value="iPhone SE"></parameter>
        <classes>
            <class name="com.herokuapp.theinternet.checkboxes.CheckboxesTests"/>
        </classes>
    </test>

</suite>